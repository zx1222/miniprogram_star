.tabbar-bottom-container {
      font-size: 26rpx;
      letter-spacing: 1px;
      font-weight: bold;
      color: #919191;
      border-top: 1px solid #8e8e90;
      background: #fff;
      display: flex;
      align-items: center;
      justify-content: space-between;
      padding: 24rpx 0;
      font-family: 'FZLTHJT'
}

.tabbar-bottom-container .item {
      flex: 1;
      text-align: center;
      border-right: 1px solid #cfcfcf;
}

.tabbar-bottom-container .item:last-child {
      border: none;
}

.selected {
      color: #f66a94;
}
